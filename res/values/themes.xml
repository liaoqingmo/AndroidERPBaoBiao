<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="logintextView">
        <item name="android:textSize">16dip</item>
        <item name="android:textColor">#6C4E34</item>
    </style>
    <style name="loginEdittext">
        <item name="android:background">@null</item>
        <item name="android:textSize">14dip</item>
    </style>
    <style name="createOrderTextStyle">
        <item name="android:textSize">19dip</item>
        <item name="android:textColor">#000000</item>
        <item name="android:width">80px</item>
    </style>
    <style name="wlDetailTextStyle">
        <item name="android:textSize">19dip</item>
        <item name="android:textColor">#000000</item>
        <item name="android:width">40px</item>
    </style>
    <style name="createOrderLyt">
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">7dip</item>
        <item name="android:paddingRight">7dip</item>
    </style>
    <style name="createOrderLytTop" parent="@style/createOrderLyt">
        <item name="android:background">@drawable/setting_top_normal</item>
        <item name="android:paddingTop">10dip</item>
    </style>
    <style name="createOrderLytMid" parent="@style/createOrderLyt">
        <item name="android:background">@drawable/setting_middle_normal</item>
        <item name="android:paddingTop">5dip</item>
    </style>
    <style name="createOrderLytBtm" parent="@style/createOrderLyt">
        <item name="android:background">@drawable/setting_bottom_normal</item>
        <item name="android:paddingTop">5dip</item>
        <item name="android:paddingBottom">5dip</item>
    </style>
    <style name="scrollList">
        <item name="android:paddingTop">5dip</item>
        <item name="android:paddingBottom">7dip</item>
        <item name="android:paddingLeft">10dip</item>
        <item name="android:paddingRight">9dip</item>
        <item name="android:scrollbars">none</item>
    </style>
</resources>